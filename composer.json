{
  "name": "alex-bond/yii2-select2-widget",
  "description": "The select2 widget for Yii framework.",
  "type": "yii2-extension",
  "keywords": [
    "yii",
    "extension",
    "widget",
    "yii2",
    "yii 2",
    "select2",
    "select 2",
    "select"
  ],
  "homepage": "https://github.com/Alex-Bond/yii2-select2-widget",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Crudu Vasile",
      "email": "bazillio07@yandex.ru",
      "homepage": "https://github.com/vova07",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Alex-Bond/yii2-select2-widget/issues?state=open",
    "source": "https://github.com/Alex-Bond/yii2-select2-widget"
  },
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "*",
    "bower-asset/select2": "4.*",
    "bower-asset/select2-bootstrap-theme": "*"
  },
  "require-dev": {
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "alexBond\\select2\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "asset-installer-paths": {
      "bower-asset-library": "vendor/bower"
    }
  }
}
